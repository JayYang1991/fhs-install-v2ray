{
  "log": {
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log",
    "loglevel": "error"
  },
  "reverse": {
    "bridges": [
      {
        "tag": "ssl_bridge",
        "domain": "private.desktop.jason.com"
      }
    ]
  },
  "inbounds": [
    {
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "tls",
          "http"
        ]
      },
      "listen": "127.0.0.1",
      "protocol": "socks",
      "settings": {
        "udp": true,
        "auth": "noauth"
      },
      "port": "1080"
    },
    {
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "tls",
          "http"
        ]
      },
      "listen": "127.0.0.1",
      "protocol": "http",
      "settings": {
        "timeout": 360
      },
      "port": "1087"
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "protocol": "vmess",
      "mux": {
        "enabled": false,
        "concurrency": 8
      },
      "streamSettings": {
        "network": "kcp",
        "kcpSettings": {
          "header": {
            "type": "dtls"
          },
          "mtu": 1350,
          "congestion": false,
          "tti": 20,
          "uplinkCapacity": 50,
          "writeBufferSize": 1,
          "readBufferSize": 1,
          "downlinkCapacity": 20
        },
        "security": "none",
        "sockopt": {
          "mark": 255
        }
      },
      "settings": {
        "vnext": [
          {
            "address": "{V2RAY_PROXY_SERVER_IP}",
            "users": [
              {
                "id": "{V2RAY_PROXY_ID}",
                "alterId": 4,
                "level": 0,
                "security": "auto"
              }
            ],
            "port": 31523
          }
        ]
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIP"
      }
    },
    {
      "tag": "tunnel",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "{V2RAY_REVERSE_SERVER_IP}",
            "port": 6688,
            "users": [
              {
                "id": "{V2RAY_REVERSE_ID}",
                "alterId": 0
              }
            ]
          }
        ]
      }
    },
    {
      "tag": "backend",
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "block",
      "protocol": "blackhole",
      "settings": {
        "response": {
          "type": "none"
        }
      }
    }
  ],
  "routing": {
    "domainStrategy": "IPOnDemand",
    "rules": [
      {
        "type": "field",
        "outboundTag": "direct",
        "domain": [
          "geosite:cn",
          "geosite:private",
          "localhost"
        ]
      },
      {
        "type": "field",
        "outboundTag": "direct",
        "ip": [
          "geoip:cn",
          "geoip:private"
        ]
      },
      {
        "type": "field",
        "inboundTag": [
          "ssl_bridge"
        ],
        "outboundTag": "tunnel",
        "domain": [
          "full:private.desktop.jason.com"
        ]
      },
      {
        "type": "field",
        "inboundTag": [
          "ssl_bridge"
        ],
        "outboundTag": "backend"
      },
      {
        "type": "field",
        "outboundTag": "proxy",
        "network": "tcp,udp"
      }
    ]
  }
}