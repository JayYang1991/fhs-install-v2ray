{
    "log": {
        "access": "/var/log/v2ray/access.log",
        "error": "/var/log/v2ray/error.log",
        "loglevel": "info"
    },
    "reverse": {
        "bridges": [
            {
                "tag": "ssl_bridge",
                "domain": "private.nas.jason.com"
            }
        ]
    },
    "outbounds": [
        {
            "tag": "tunnel",
            "protocol": "vmess",
            "settings": {
                "vnext": [
                    {
                        "address": "V2RAY_REVERSE_SERVER_IP",
                        "port": 6688,
                        "users": [
                            {
                                "id": "{V2RAY_REVERSE_ID}",
                                "alterId": 0
                            }
                        ]
                    }
                ]
            }
        },
        {
            "tag": "backend",
            "protocol": "freedom",
            "settings": {}
        }
    ],
    "routing": {
        "settings": {
            "domainStrategy": "AsIs",
            "rules": [
                {
                    "type": "field",
                    "inboundTag": [
                        "ssl_bridge"
                    ],
                    "outboundTag": "tunnel",
                    "domain": [
                        "full:private.nas.jason.com"
                    ]
                },
                {
                    "type": "field",
                    "inboundTag": [
                        "ssl_bridge"
                    ],
                    "outboundTag": "backend"
                }
            ]
        }
    }
}